<body">
    <nav class="navbar navbar-inverse navbar-fixed-top"></nav>

    <div class="imgcontainer">
        <img src="images/default-user.png" alt="Avatar" class="avatar">
    </div>

    <form role="form" method="POST" action="/login">

        <label for="uname"><h1>Username</h1></label>
        <input type="text" placeholder="Enter Username" name="uname" required>

        <label for="psw"><h1>Password</h1></label>
        <input type="password" placeholder="Enter Password" name="psw" required>

        <button type="submit">Login</button>
     
        <input type="checkbox" checked="checked" name="remember">
        <label><h2>Remember me</h2></label>
    </form>

      <div class="pswcontainer" style="background-color:#f1f1f1">
        <span class="psw"><a href="#"><h2>Forgot password?</h2></a></span>
      </div>


  
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
